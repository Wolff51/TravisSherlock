{
  "name": "sherlock-studio",
  "author": {
    "name": "Supairvision",
    "email": "lw@supairvision.com"
  },
  "version": "0.4.2",
  "description": "Sherlock Studio Beta",
  "private": true,
  "dependencies": {
    "@craco/craco": "^7.0.0",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "add": "^2.0.6",
    "ansi-regex": "^6.0.1",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "csv-reader": "^1.0.11",
    "dotenv": "^16.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-packager": "^17.1.1",
    "exif-parser": "^0.1.12",
    "fs": "^0.0.1-security",
    "hoist-non-react-statics": "^3.3.2",
    "jsonwebtoken": "^9.0.0",
    "markerjs2": "^2.29.1",
    "mssql": "^9.1.1",
    "mysql2": "^3.0.1",
    "papaparse": "^5.4.1",
    "path": "^0.12.7",
    "pg": "^8.8.0",
    "postinstall": "^0.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-idle-timer": "^5.5.2",
    "react-image-crop": "^10.0.9",
    "react-is": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1",
    "react-scripts": "5.0.1",
    "react-use": "^17.4.0",
    "react-zoom-pan-pinch": "^3.0.8",
    "redux": "^4.2.1",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.19"
  },
  "main": "./public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electrondev": "set ELECTRON_IS_DEV=1 && electron .",
    "electron-trace": "electron --trace-warnings .",
    "electron:package:win": "yarn build && electron-builder -w -c.extraMetadata.main=build/electron.js && cp -R python build/",
    "electron:package:mac": "yarn build && electron-builder -m -c.extraMetadata.main=build/electron.js && cp -R python build/",
    "electron:package:linux": "yarn build && electron-builder -l -c.extraMetadata.main=build/electron.js && cp -R python build/"
  },
  "build": {
    "appId": "com.electron.myapp",
    "productName": "Sherlock Studio Beta",
    "extraResources": [
      {
        "from": "python",
        "to": "python"
      }
    ],
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "icon": "public/images/icone.png",
      "target": "dmg"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "icon": "public/images/icone.png",
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE.txt"
    },
    "linux": {
      "icon": "public/images/icone.png",
      "target": "deb"
    }
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.10",
    "autoprefixer": "^10.4.13",
    "electron": "^22.0.0",
    "electron-builder": "^24.4.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-installer-dmg": "^4.0.0",
    "electron-is-dev": "^2.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
