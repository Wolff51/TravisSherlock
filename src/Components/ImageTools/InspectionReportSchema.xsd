      <xs:enumeration value="Shear Web C" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="de_u_blade_sub_section" final="restriction" >
    <xs:restriction base="xs:string">
      <xs:enumeration value="Bonding" />
